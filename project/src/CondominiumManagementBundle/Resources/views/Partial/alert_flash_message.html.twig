{% for flashMessage in app.session.flashbag.get('error') %}
    <div class="alert alert-danger" role="alert">
        <button
            type="button"
            class="close"
            data-dismiss="alert"
            aria-label="Close"
        >
            <span aria-hidden="true">&times;</span>
        </button>
        <strong>{{ 'label.error'|trans }}!</strong>
        {{ flashMessage }}
    </div>
{% endfor %}
